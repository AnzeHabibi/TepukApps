<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.tepukapps">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme"
        android:usesCleartextTraffic="true">
        <activity android:name=".DetailsHistory"></activity>
        <activity
            android:name=".History"
            android:label="@string/title_activity_history"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".HomeActivity"
            android:launchMode="singleTask" />
        <activity android:name=".SuccesBuy" />
        <activity android:name=".TrackingAct" />
        <activity android:name=".CartActivity" />
        <activity android:name=".PaymentAct" />
        <activity android:name=".SignUp2" />
        <activity android:name=".SuccerRegister" />
        <activity android:name=".DetailAct" />
        <activity android:name=".Signup" />
        <activity android:name=".Signin" />
        <activity android:name=".GetStarted" />
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>